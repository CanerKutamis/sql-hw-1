 Cevap 1-)
Select rating from film
group by rating;

 Cevap 2-)
Select replacement_cost,count(*) from film 
group by replacement_cost
having count(*)>50;

 Cevap 3-)
Select store_id,count(*) from customer 
group by store_id;

 Cevap 4-)
Select country_id,count(*) from city
group by country_id
order by count(city) desc
limit 1;
